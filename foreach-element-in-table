// prosty program z pętlą foreach i tablicą
// pętla foreach służy do odczytywania danych (elementu) z tablicy (po prostu) , indeks odczytywanego elementu
// sam rośnie w pętli


using System;
using System.IO;
public class program {
public static void Main()
{
int[] tablica = { 1, 2, 3, 4 };
foreach (int x in tablica)
    Console.WriteLine(x);
    
}
 
}
